option_settings:
  - option_name: PROD 
    value: True
  - option_name: ALLOWED_HOSTS
    value: "*"
container_commands:
  01migrate:
    command: "docker exec `docker ps -a --no-trunc -q | head -n 1` /usr/bin/python3 /home/docker/code/manage.py migrate --noinput  &>> /tmp/deploy.log"
    leader_only: true
